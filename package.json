{
  "name": "@libsrcdev/gatsby-remark-structured-content",
  "version": "0.1.4",
  "description": "This plugin helps building thumbnails, covers, heros, carousel features from remark content.",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "extract",
    "remark",
    "images",
    "thumbnail",
    "cover",
    "hero",
    "carousel"
  ],
  "homepage": "https://github.com/libsrcdev/gatsby-remark-structured-content",
  "bugs": {
    "url": "https://github.com/libsrcdev/gatsby-remark-structured-content/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libsrcdev/gatsby-remark-structured-content.git"
  },
  "license": "MIT",
  "author": "@libsrcdev",
  "type": "module",
  "main": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nodemon --watch src  --ext js,ts,mjs,cjs,json --exec \"tsdown\"",
    "build": "tsdown",
    "lint": "eslint . --ext .ts",
    "test": "vitest",
    "prepare": "npm run build"
  },
  "dependencies": {
    "gatsby-source-filesystem": "^5.15.0",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.15.3",
    "@types/mdast": "^4.0.4",
    "eslint": "^9.39.1",
    "gatsby": "^5.15.0",
    "husky": "^9.1.7",
    "mdast": "^2.3.2",
    "nodemon": "^3.1.11",
    "tsdown": "^0.16.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "typings": "dist/index.d.ts",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./gatsby-node": {
      "import": "./dist/gatsby-node.mjs",
      "require": "./dist/gatsby-node.cjs"
    },
    "./package.json": "./package.json",
    "./foo": "./foo.js"
  }
}
